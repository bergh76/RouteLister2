                                    <thead>
                                        <tr class="text-center">
                                            <th><h4>Antal</h4></th>
                                            <th><h4>KollieId</h4></th>
                                            <th><h4>Artikel</h4></th>
                                            <th><h4>Plockad</h4></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td><h5>1</h5></td>
